{
  "private": true,
  "packageManager": "pnpm@6.32.3",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint --ext .js,.jsx,.vue,.ts,.tsx src",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^8.9.4",
    "vue": "^3.2.37",
    "vue-router": "^4.1.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.9",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@iconify-json/carbon": "^1.1.7",
    "@types/node": "^17.0.45",
    "@unocss/reset": "^0.27.6",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vue/test-utils": "^2.0.2",
    "eslint": "^8.22.0",
    "husky": ">=8.0.1",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.5.0",
    "pnpm": "^6.34.0",
    "typescript": "^4.7.4",
    "unocss": "^0.27.6",
    "unplugin-auto-import": "^0.6.9",
    "unplugin-vue-components": "^0.17.21",
    "vite": "^3.0.9",
    "vitest": "^0.6.3",
    "vue-tsc": "^0.32.1"
  },
  "eslintConfig": {
    "extends": "@antfu",
    "rules": {
      "no-console": "off"
    }
  },
  "lint-staged": {
    "*.{vue,ts,tsx}": [
      "npm run lint"
    ]
  }
}
